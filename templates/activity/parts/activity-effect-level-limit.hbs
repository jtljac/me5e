<div class="form-group">
    <label>{{ localize "ME5E.ACTIVITY.FIELDS.effects.element.level.label" }}</label>
    <div class="form-fields range-fields">
        {{ formInput fields.level.fields.min name=(concat prefix "level.min") value=data.level.min
                     placeholder="0" ariaLabel=(localize "ME5E.LevelLimit.Min")
                     input=@root.inputs.createNumberInput }}
        <me5e-icon src="systems/me5e/icons/svg/range-connector.svg" class="sep"></me5e-icon>
        {{ formInput fields.level.fields.max name=(concat prefix "level.max") value=data.level.max
                     placeholder="âˆž" ariaLabel=(localize "ME5E.LevelLimit.Max") }}
    </div>
    <p class="hint">{{ localize "ME5E.ACTIVITY.FIELDS.effects.element.level.hint" }}</p>
</div>
